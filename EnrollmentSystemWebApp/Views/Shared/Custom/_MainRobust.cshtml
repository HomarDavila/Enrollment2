    <script>
        
        $.ajaxSetup({ cache: false });
        $(document).ready(function () {
		    var UserSettings = {
			    language: "@Session[SessionHelper.LANGUAGE_SESSION_KEY]",
			    culture: "@Session[SessionHelper.CULTURE_SESSION_KEY]"
		    }
		    //var Logging = log.noConflict();
		    //Logging.enableAll();
            String.language = UserSettings.language;
		    String.defaultLocale = "@AppConstants.DefaultCulture";
            String.locale = UserSettings.culture;

            

            //how to stop browser back button
		    //window.location.hash = "no-back-button";
		    //window.location.hash = "Again-No-back-button";//again because google chrome don't insert first hash into history
      //      window.onhashchange = function () { window.location.hash = "no-back-button"; };
        });
        function changeLanguage(urlLanguage, language) {
            debugger
                swal({
                    title: i18next.t('common_reloadTitel1'),
                    text: i18next.t('common_reloadSubTitle1'),
                    type: "warning",
                    showCancelButton: true,
                    confirmButtonClass: "btn-danger",
                    confirmButtonText: i18next.t('common_reloadButtonOK'),
                    cancelButtonText: i18next.t('common_reloadButtonCancel'),
                    closeOnConfirm: true
                },
                    function () {
                    //i18next.changeLanguage(language);
                    //window.location.reload(true);
                     $.ajax({
                        url: '@Url.Action("ChangeLanguage", "Common")',
                        dataType: 'json',
                        data: {
                            language: language
                        },
                        type: 'GET',
                        async: false,
                         success: function (response) {
                             i18next.changeLanguage(language);

                            ChangeLanguajeFiles();
                             $('#results-div table').DataTable({
                                destroy:true,
                                language: {
                                    "decimal": ".",
                                    "sEmptyTable": i18next.t("datatable_empty"),
                                    "sInfo": i18next.t("datatable_info"),
                                    "sLengthMenu": i18next.t("datatable_LengthMenu"),
                                    "oPaginate": {
                                        "sFirst": i18next.t("datatable_first"),
                                        "sPrevious": i18next.t("datatable_previous"),
                                        "sNext": i18next.t("datatable_next"),
                                        "sLast": i18next.t("datatable_last")
                                    }
                                },
                                "ordering": true,
                                "processing": true, // for show progress bar
                                "filter": false, // this is for disable filter (search box)
                                "orderMulti": false, // for disable multiple column at once
                                "columnDefs":
                                    [
                                        {
                                            "targets": [0],
                                            "visible": true,
                                            "orderable": true
                                        }
                                    ],
                                dom: 'lBfrtip',
                                buttons: [

                                ]
                             });
                          
                        },
                        error: function (XMLHttpRequest, textStatus, errorThrown) {
                            alert('Occurrió un error en el translate')
                        }
                    });
                    });

        }

        function ChangeLanguajeFiles() {

            $('#tbFiles, #tbHistorySearch').DataTable({
                                destroy:true,
                                language: {
                                    "decimal": ".",
                                    "sEmptyTable": i18next.t("datatable_empty"),
                                    "sInfo": i18next.t("datatable_info"),
                                    "sLengthMenu": i18next.t("datatable_LengthMenu"),
                                    "oPaginate": {
                                        "sFirst": i18next.t("datatable_first"),
                                        "sPrevious": i18next.t("datatable_previous"),
                                        "sNext": i18next.t("datatable_next"),
                                        "sLast": i18next.t("datatable_last")
                                    }
                                },
                                "autoWidth": true,
                                "scrollX": true,
                                "paging": false,
                                "ordering": true,
                                "processing": true, // for show progress bar
                                "filter": false, // this is for disable filter (search box)
                                "orderMulti": false, // for disable multiple column at once
                                "columnDefs":
                                    [
                                        {
                                            "targets": [0],
                                            "visible": true,
                                            "orderable": false
                                        }
                                    ],
                            } );
        }
    </script>
